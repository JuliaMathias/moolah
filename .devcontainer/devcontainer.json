{
  "name": "Moolah",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspaces/moolah",
  "features": {
    "ghcr.io/devcontainers/features/node:1": {
      "version": "24"
    }
  },
  "forwardPorts": [
    4000,
    5432
  ],
  "remoteEnv": {
    "MIX_ENV": "dev",
    "DB_HOST": "db"
  },
  "postCreateCommand": "bash .devcontainer/postCreate.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "vscode.theme-abyss",
        "vscode.theme-defaults",
        "vscode.theme-kimbie-dark",
        "vscode.theme-monokai",
        "vscode.theme-monokai-dimmed",
        "vscode.theme-red",
        "vscode.theme-solarized-dark",
        "vscode.theme-solarized-light",
        "vscode.theme-tomorrow-night-blue",
        "adamzapasnik.elixir-test-explorer",
        "adpyke.vscode-sql-formatter",
        "alexdauenhauer.catppuccin-noctis",
        "aliariff.vscode-erb-beautify",
        "animus-coop.vscode-elixir-mix-formatter",
        "anthropic.claude-code",
        "arcticicestudio.nord-visual-studio-code",
        "benvp.vscode-hex-pm-intellisense",
        "castwide.solargraph",
        "christian-kohler.npm-intellisense",
        "clinyong.vscode-css-modules",
        "daltonmenezes.aura-theme",
        "davidanson.vscode-markdownlint",
        "dbaeumer.vscode-eslint",
        "docker.docker",
        "dotjoshjohnson.xml",
        "dracula-theme.theme-dracula",
        "dunstontc.vscode-elixir-syntax",
        "eamodio.gitlens",
        "enkia.tokyo-night",
